{
    "_version": "1.7.0",
    "sap.app": {
        "id": "com.infocus.vendorAdvance",
        "type": "application",
        "i18n": "i18n/i18n.properties",
        "applicationVersion": {
            "version": "1.0.0"
        },
        "title": "{{appTitle}}",
        "description": "{{appDescription}}",
        "sourceTemplate": {
            "id": "servicecatalog.connectivityComponent",
            "version": "0.0.0"
        },
        "dataSources": {
            "mainService": {
                "uri": "/sap/opu/odata/sap/ZO_VEND_HISTORY_SRV/",
                "type": "OData",
                "settings": {
                    "odataVersion": "2.0",
                    "localUri": "localService/metadata.xml"
                }
            },
            "ZVENDOR_ADVANCE_SRV": {
                "uri": "/sap/opu/odata/sap/ZVENDOR_ADVANCE_SRV/",
                "type": "OData",
                "settings": {
                    "odataVersion": "2.0",
                    "localUri": "localService/ZVENDOR_ADVANCE_SRV/metadata.xml"
                }
            },
            "ZVENDOR_summary_SRV": {
                "uri": "/sap/opu/odata/sap/ZVENDOR_summary_SRV/",
                "type": "OData",
                "settings": {
                    "odataVersion": "2.0",
                    "localUri": "localService/ZVENDOR_summary_SRV/metadata.xml"
                }
            },
            "Z_INF_VEND_ANAL_SRV": {
                "uri": "/sap/opu/odata/sap/Z_INF_VEND_ANAL_SRV/",
                "type": "OData",
                "settings": {
                    "odataVersion": "2.0",
                    "localUri": "localService/Z_INF_VEND_ANAL_SRV/metadata.xml"
                }
            }
        }
    },
    "sap.ui": {
        "technology": "UI5",
        "icons": {
            "icon": "",
            "favIcon": "",
            "phone": "",
            "phone@2": "",
            "tablet": "",
            "tablet@2": ""
        },
        "deviceTypes": {
            "desktop": true,
            "tablet": true,
            "phone": true
        },
        "supportedThemes": [
            "sap_hcb",
            "sap_belize"
        ]
    },
    "sap.ui5": {
        "rootView": {
            "viewName": "com.infocus.vendorAdvance.view.App",
            "type": "XML"
        },
        "dependencies": {
            "minUI5Version": "1.30.0",
            "libs": {
                "sap.collaboration": {},
                "sap.m": {},
                "sap.suite.ui.commons": {},
                "sap.ui.comp": {},
                "sap.ui.core": {},
                "sap.ui.layout": {},
                "sap.ushell": {},
                "sap.uxap": {}
            }
        },
        "contentDensities": {
            "compact": true,
            "cozy": true
        },
        "models": {
            "i18n": {
                "type": "sap.ui.model.resource.ResourceModel",
                "settings": {
                    "bundleName": "com.infocus.vendorAdvance.i18n.i18n"
                }
            },
            "": {
                "dataSource": "mainService",
                "preload": true,
                "settings": {
                    "useBatch": true,
                    "defaultBindingMode": "TwoWay"
                }
            },
            "ZVENDOR_ADVANCE_SRV": {
                "dataSource": "ZVENDOR_ADVANCE_SRV",
                "settings": {
                    "useBatch": false,
                    "defaultBindingMode": "TwoWay"
                }
            },
            
            "Z_INF_VEND_ANAL_SRV": {
                "dataSource": "Z_INF_VEND_ANAL_SRV",
                "settings": {
                    "useBatch": false,
                    "defaultBindingMode": "TwoWay"
                }
            },
            "globalData": {
                "type": "sap.ui.model.json.JSONModel"
            },
            "stillToDeliverData": {
                "type": "sap.ui.model.json.JSONModel"
            },
            "venderHistoryData": {
                "type": "sap.ui.model.json.JSONModel"
            },
            "venderMasterData": {
                "type": "sap.ui.model.json.JSONModel"
            },
            "companyCodeMasterData": {
                "type": "sap.ui.model.json.JSONModel"
            },
            "accountGroupData": {
                "type": "sap.ui.model.json.JSONModel"
            },
            "plantData":{
            	"type": "sap.ui.model.json.JSONModel"
            },
            "vendorAdvanceData": {
                "type": "sap.ui.model.json.JSONModel"
            },
            "totalVendorAdvanceData": {
                "type": "sap.ui.model.json.JSONModel"
            },
            "top10VendorAdvanceData": {
                "type": "sap.ui.model.json.JSONModel"
            },
            "creditorData": {
                "type": "sap.ui.model.json.JSONModel"
            },
            "totalCreditorData": {
                "type": "sap.ui.model.json.JSONModel"
            },
            "top10CreditorData": {
                "type": "sap.ui.model.json.JSONModel"
            },
            "lcData": {
                "type": "sap.ui.model.json.JSONModel"
            },
            "top10LCData": {
                "type": "sap.ui.model.json.JSONModel"
            },
            "totalLCData": {
                "type": "sap.ui.model.json.JSONModel"
            },
            "factoringData": {
                "type": "sap.ui.model.json.JSONModel"
            },
            "top10FactoringData": {
                "type": "sap.ui.model.json.JSONModel"
            },
            "totalFactoringData": {
                "type": "sap.ui.model.json.JSONModel"
            },
            "rxilData": {
                "type": "sap.ui.model.json.JSONModel"
            },
            "top10RXILData": {
                "type": "sap.ui.model.json.JSONModel"
            },
            "totalRXILData": {
                "type": "sap.ui.model.json.JSONModel"
            },
            "abgData": {
                "type": "sap.ui.model.json.JSONModel"
            },
            "top10ABGData": {
                "type": "sap.ui.model.json.JSONModel"
            },
            "totalABGData": {
                "type": "sap.ui.model.json.JSONModel"
            },
            "pbgData": {
                "type": "sap.ui.model.json.JSONModel"
            },
            "top10PBGData": {
                "type": "sap.ui.model.json.JSONModel"
            },
            "totalPBGData": {
                "type": "sap.ui.model.json.JSONModel"
            },
            "selectedPanelModel": {
                "type": "sap.ui.model.json.JSONModel",
                "data": {
                    "selectedPanel": "panel1"
                }
            },
            "summaryData": {
                "type": "sap.ui.model.json.JSONModel"
            }
        },
        "resources": {
            "css": [
                {
                    "uri": "css/style.css"
                }
            ],
            "js": []
        },
        "routing": {
            "config": {
                "routerClass": "sap.m.routing.Router",
                "viewType": "XML",
                "async": true,
                "viewPath": "com.infocus.vendorAdvance.view",
                "controlId": "app",
                "controlAggregation": "pages",
                "clearAggregation": false
            },
            "routes": [
                {
                    "name": "home",
                    "pattern": "",
                    "target": "home"
                },
                {
                    "name": "chart",
                    "pattern": "chart",
                    "target": "chart"
                }
            ],
            "targets": {
                "home": {
                    "viewName": "Home",
                    "viewLevel": 1
                },
                "chart": {
                    "viewName": "Chart",
                    "viewLevel": 2
                }
            }
        }
    },
    "sap.platform.abap": {
        "uri": "/sap/bc/ui5_ui5/sap/zven_analysis",
        "_version": "1.1.0"
    }
}