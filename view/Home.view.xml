							<DatePicker id="_venderDatePickerId" value="{globalData>/selectedVenderDate}" placeholder="Select Due Date" required="true"
								tooltip="Select from list" class="boldText roundedInput" change="onVenderDateChange" displayFormat="dd.MM.yyyy" valueFormat="yyyyMMdd"/>
						</HBox>
						<HBox id="reportTypeBox" direction="Column" alignItems="Center" visible="true" class="sapUiTinyMarginBegin">
							<Select id="_reportTypeSelect" forceSelection="true" selectedKey="{selectedPanelModel>/selectedPanel}" change=".handleReportTypeChange"
